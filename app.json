{
  "expo": {
    "name": "Timer",
    "slug": "timer",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.adithyanr.timer"
    },
    "android": {
      "package": "com.adithyanr.timer"
    },
    "description": "A simple timer app built with React native",
    "githubUrl": "https://github.com/AdithyaBhat17/timer-app"
  }
}